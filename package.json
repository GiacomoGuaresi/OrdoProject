{
  "name": "ordoproject",
  "version": "1.0.0",
  "main": "electron/main.js",
  "build": {
    "appId": "com.ordo.project",
    "productName": "OrdoProject",
    "files": [
      "dist/**",
      "electron/**",
      "node_modules/**",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "icon": "build/icon.ico",
      "target": "portable"
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "linux": {
      "icon": "build/icon.png"
    }
  },
  "scripts": {
    "dev": "vite",
    "start": "electron .",
    "dev:all": "concurrently \"npm run dev\" \"npm run start\"",
    "build": "vite build && electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "date-fns": "^4.1.0",
    "electron-is-dev": "^3.0.1",
    "minimatch": "^10.0.3",
    "react-sortablejs": "^6.1.4",
    "sortablejs": "^1.15.6"
  }
}
